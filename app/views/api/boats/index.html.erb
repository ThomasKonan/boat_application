<h1>I am in index.html</h1>

<% @boats.each do |boat| %>
  <p><%= boat.title %></p>
  <p><%= boat.capacity %></p>
  <p><%= boat.color %></p>
  <p><%= boat.year_christened %></p>
  <p><%= boat.price %></p>
  <p>title: <a href="/boats/<%= boat.id %>"><%= boat.title%></a></p>
  <p>capacity: <%= boat.capacity %></p>
  <hr>
<% end %>